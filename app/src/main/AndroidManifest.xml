<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.catbreedsquizgame">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.QuizGame">
        <activity
            android:name="com.example.catbreedsquizgame.QuizActivity"
            android:exported="false" />
        <activity
            android:name="com.example.catbreedsquizgame.ResultActivity"
            android:exported="false" />
        <activity
            android:name="com.example.catbreedsquizgame.SplashScreenActivity"
            android:exported="true"
            android:theme="@style/Theme.Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.catbreedsquizgame.MainActivity"
            android:exported="true" />
    </application>

</manifest>